\begin{lstlisting}

# Create a stem plot
plt.stem(n_values, y_values, linefmt='|', markerfmt='o', basefmt='b', label='Stem Plot')

plt.xlabel('n_values')
plt.ylabel('y_values')
plt.title('Stem Plot of n_values vs y_values')
plt.grid(True)
plt.legend()

plt.savefig('figs/fig1.png')
\end{lstlisting}
